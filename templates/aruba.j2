hostname "RHCEC-IDF1"

timesync ntp
ntp unicast
ntp server 10.101.0.10 iburst
ntp server 10.101.0.11 iburst
ntp enable
time daylight-time-rule continental-us-and-canada
time timezone -300
ip default-gateway 10.57.2.1
ip dns server-address priority 1 10.101.0.10
snmp-server community "public" unrestricted
vlan 1
   name "DEFAULT_VLAN"
   no untagged 1-48
   untagged 49-52
   ip address dhcp-bootp
   ipv6 enable
   ipv6 address dhcp full
   exit
vlan 128
   name "SECURE"
   tagged 1,43-48,51-52
   no ip address
   exit
vlan 192
   name "GUEST"
   tagged 1,43-48,51-52
   no ip address
   exit
vlan 402
   name "SW_MGMT"
   tagged 51
   ip address 10.57.2.11 255.255.255.0
   exit
vlan 403
   name "AP_MGMT"
   untagged 43-48
   tagged 51-52
   no ip address
   exit
vlan 404
   name "VOIP"
   tagged 51-52
   no ip address
   voice
   exit
vlan 405
   name "Eternal_AP_MGMT"
   untagged 1
   tagged 51-52
   no ip address
   exit
vlan 406
   name "SECURITY"
   tagged 51-52
   no ip address
   exit
vlan 407
   name "IOT"
   tagged 51-52
   no ip address
   exit
vlan 412
   name "VIDEO"
   tagged 51
   no ip address
   exit
vlan 431
   name "IDF_DATA"
   untagged 2-42
   tagged 51
   no ip address
   exit
spanning-tree
spanning-tree 51 priority 4
spanning-tree 52 priority 4
spanning-tree force-version rstp-operation
no tftp server
no autorun
no dhcp config-file-update
no dhcp image-file-update
no dhcp tr69-acs-url
device-identity name "VOIP-Phone"
   lldp sys-name "Polycom"
   exit
device-profile name "Phone-Profile"
   untagged-vlan 431
   tagged-vlan 404
   exit
device-profile device-type "VOIP-Phone"
   associate "Phone-Profile"
   enable
   exit
